{
  "name": "struggle-for-php/sfp-deptrac-builtin",
  "description": "Deptrac extension to enforce application on framework rule.",
  "keywords": ["deptrac", "header", "static analysis", "static-analysis"],
  "license": "MIT",
  "authors": [
    {
      "name": "sasezaki",
      "email": "sasezaki@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4 || ~8.0.0 || ~8.1.0",
    "qossmic/deptrac-shim": "^1"
  },
  "require-dev": {
    "bamarni/composer-bin-plugin": "^1.8",
    "laminas/laminas-coding-standard": "^2.4",
    "phpunit/phpunit": "^9.5",
    "psalm/plugin-phpunit": "^0.17.0",
    "squizlabs/php_codesniffer": "^3.7",
    "vimeo/psalm": "^4.27"
  },
  "autoload": {
      "psr-4": {
        "Sfp\\Deptrac\\Builtin\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "SfpTest\\Deptrac\\Builtin\\": "tests"
      }
    },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bamarni/composer-bin-plugin": true
    },
      "platform": {
        "php": "8.1.99"
    }
  }
}
